# MinTid Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Google Analytics 4
VITE_GA_MEASUREMENT_ID=GA_MEASUREMENT_ID

# Sentry Error Tracking  
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Application Configuration
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development
VITE_APP_URL=https://mintid.netlify.app
VITE_APP_NAME=MinTid

# SEO Configuration
VITE_CANONICAL_URL=https://mintid.netlify.app
VITE_OG_IMAGE=https://mintid.netlify.app/og-image.png

# API Configuration (for future use)
VITE_API_URL=https://api.mintid.com

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_SEO=true
VITE_DEMO_MODE=true
